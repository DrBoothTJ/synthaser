{
	"rules": [
		{
			"name": "Hybrid",
			"domains": ["KS", "A"],
			"evaluator": "0 and 1"
		},
		{
			"name": "PKS",
			"domains": ["KS"],
			"filters": {"KS": ["PKS_KS", "PKS", "CHS_like"]},
			"evaluator": "0"
		},
		{
			"name": "Multi-modular PKS",
			"domains": ["KS", "KS"],
			"evaluator": "0 and 1"
		},
		{
			"name": "Type I PKS",
			"domains": ["AT"],
			"filters": {"AT": ["PKS_AT", "Acyl_transf_1"]},
			"evaluator": "0"
		},
		{
			"name": "HR-PKS",
			"domains": ["DH", "ER", "KR"],
			"filters": {
				"DH": ["PKS_DH", "PS-DH"],
				"ER": ["PKS_ER", "enoyl_red"],
				"KR": ["PKS_KR", "KR"]
			},
			"evaluator": "all([0, 1, 2])"
		},
		{
			"name": "PR-PKS",
			"domains": ["DH", "ER", "KR"],
			"filters": {
				"DH": ["PKS_DH", "PS-DH"],
				"ER": ["PKS_ER", "enoyl_red"],
				"KR": ["PKS_KR", "KR"]
			},
			"evaluator": "any([0, 1, 2])"
		},
		{
			"name": "NR-PKS",
			"domains": ["SAT", "PT"],
			"evaluator": "0"
		},
		{
			"name": "Type III PKS",
			"domains": ["KS"],
			"filters": {"KS": ["CHS_like"]},
			"evaluator": "0"
		},
		{
			"name": "NRPS",
			"domains": ["A", "T", "ACP", "C"],
			"evaluator": "0 and (1 or 2) and 3"
		},
		{
			"name": "NRPS-like",
			"domains": ["A", "C"],
			"evaluator": "any([0, 1])"
		},
		{
			"name": "PKS-NRPS",
			"domains": ["KS", "A", "C"],
			"evaluator": "0 and (1 or 2)"
		},
		{
			"name": "FAS",
			"domains": ["KS"],
			"filters": {"KS": ["FabB", "KAS_I_II", "KAS_III", "fabF"]},
			"evaluator": "0"
		},
		{
			"name": "alpha subunit",
			"domains": ["AT", "KR", "PPT"],
			"filters": {
				"MPT": ["FabD"],
				"ACP": ["FabD"],
				"KR": ["KR_fFAS_SDR_c_like"],
				"PPT": ["AcpS", "ACPS", "acpS"]
			},
			"evaluator": "all([0, 1, 2, 3])"
		},
		{
			"name": "beta subunit",
			"domains": ["AT", "ER", "DH", "AT"],
			"filters": {
				"ER": ["COG4981"],
				"DH": ["FAS_MaoC"],
				"AT": ["FabD"]
			},
			"evaluator": "all([0, 1, 2, 3])"
		},
		{
			"name": "HMG-CoA synthase",
			"domains": ["KS"],
			"filters": {
				"KS": ["HMG-CoA-S_euk"]
			},
			"evaluator": "0"
		},
		{
			"name": "Thiolase",
			"domains": ["KS"],
			"filters": {
				"KS": ["SCP-x_thiolase", "AcCoA-C_Actrans", "thiolase", "PLN02287"]
			},
			"evaluator": "0"
		}
	],
	"graph": [
		"Hybrid",
		{
			"PKS": [
				"Multi-modular PKS",
				{"Type I PKS": ["HR-PKS", "PR-PKS", "NR-PKS"]},
				"Type III PKS"
			]
		},
		{
			"FAS": [
				"alpha subunit",
				"beta subunit",
				"HMG-CoA synthase",
				"Thiolase"
			]
		},
		"NRPS",
		"NRPS-like"
	]
}
